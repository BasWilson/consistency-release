{
  "name": "consistency",
  "version": "0.1.0",
  "main": "main.js",
  "dependencies": {
    "cookies-js": "^1.2.3",
    "electron-titlebar": "0.0.3",
    "express": "^4.16.2",
    "http": "0.0.0"
  },
  "devDependencies": {
    "electron": "^1.7.9",
    "electron-packager": "^10.1.0"
  },
  "description": "A revolutionary task list application",
  "scripts": {
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=app/icon.icns --prune=true --out=release-builds-mac",
    "package-win": "electron-packager . Consistency --overwrite --asar=true --platform=win32 --arch=ia32 --icon=app/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Consistency\"",
    "package-linux": "electron-packager . Consistency --overwrite --asar=true --platform=linux --arch=x64 --prune=true --out=release-builds"
  },
  "author": "Squadron Interactive",
  "license": "ISC"
}
